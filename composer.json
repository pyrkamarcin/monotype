{
  "name": "pyrkamarcin/monotype",
  "license": "proprietary",
  "type": "project",
  "description": "",
  "autoload": {
    "psr-4": {
      "Monotype\\": "src/Monotype"
    },
    "classmap": [
      "app/AppKernel.php",
      "app/AppCache.php"
    ]
  },
  "require": {
    "doctrine/doctrine-bundle": "1.*",
    "doctrine/doctrine-cache-bundle": "1.*",
    "doctrine/orm": "2.*",
    "incenteev/composer-parameter-handler": "2.*",
    "sensio/distribution-bundle": "5.*",
    "sensio/framework-extra-bundle": "3.*",
    "symfony/assetic-bundle": "2.*",
    "symfony/monolog-bundle": "2.*",
    "symfony/swiftmailer-bundle": "2.*",
    "symfony/symfony": "3.*",
    "symfony/security-acl": "3.*",
    "react/socket-client": "dev-master",
    "react/http": "0.4.*",
    "ramsey/uuid": "3.*",
    "ramsey/uuid-doctrine": "1.*",
    "ramsey/uuid-console": "1.*",
    "moontoast/math": "1.*",
    "mockery/mockery": "^0.9",
    "phpunit/phpunit": "^5.7",
    "roave/security-advisories": "dev-master"
  },
  "require-dev": {
    "sensio/generator-bundle": "3.*",
    "symfony/phpunit-bridge": "3.*"
  },
  "scripts": {
    "post-install-cmd": [
      "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
      "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
      "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
      "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
      "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile",
      "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::prepareDeploymentTarget"
    ],
    "post-update-cmd": [
      "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
      "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
      "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
      "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
      "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile",
      "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::prepareDeploymentTarget"
    ]
  },
  "config": {
    "platform": {
      "php": "7.0"
    }
  },
  "extra": {
    "symfony-app-dir": "app",
    "symfony-bin-dir": "bin",
    "symfony-var-dir": "var",
    "symfony-assets-install": "relative",
    "incenteev-parameters": {
      "file": "app/config/parameters.yml"
    }
  }
}
